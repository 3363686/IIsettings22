<application>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JSP" value="true" />
        <entry key="JSPX" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="PHP" value="true" />
        <entry key="Python" value="true" />
        <entry key="Stylus" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="1" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="todo.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bask-deleted-ame\b.*" />
    <pattern icon="question" pattern="_ame_.*" />
    <pattern icon="question" useCustomColors="true" pattern="^#+ +\d+.*">
      <option name="FOREGROUND" value="bb0000" />
      <option name="ERROR_STRIPE_COLOR" value="ffaaaa" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="hdr.*">
      <option name="FOREGROUND" value="cd00f3" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="cd00f3" />
    </pattern>
    <filter name="CSV cat#">
      <pattern index="4" />
    </filter>
    <filter name="&lt;th&gt; list">
      <pattern index="5" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingLevelCount" value="8" />
  </component>
</application>